{
    "driver": {
        "num": 99,
        "typename": "simpixel"
    },

    "layout": {
        "typename": "circle",
        "pixels_per": [1, 4, 8, 12, 18, 24, 32]
    },

    "run": {
        "fps": 30,
        "seconds": 12
    },

    "animation": {
        "typename": "sequence",
        "animations": [
            "BiblioPixelAnimations.circle.arc_clock.ArcClock",
            "BiblioPixelAnimations.circle.arc_rotate.ArcRotate",
            "BiblioPixelAnimations.circle.bloom.CircleBloom",
            "BiblioPixelAnimations.circle.circle_clock.CircleClock",
            "BiblioPixelAnimations.circle.diag.Diag",
            "BiblioPixelAnimations.circle.fireflies.FireFlies",
            "BiblioPixelAnimations.circle.hyperspace.Hyperspace",
            "BiblioPixelAnimations.circle.hyperspace.HyperspaceRainbow",
            "BiblioPixelAnimations.circle.pinwheel.PinWheel",
            "BiblioPixelAnimations.circle.swirl.Swirl"
        ]
    }
}
